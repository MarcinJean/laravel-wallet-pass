{
  "name": "marcinjean/laravel-wallet-pass",
  "description": "Generate and manage Apple & Google Wallet passes in Laravel.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Marcin Jean",
      "email": "Marcin@MarcinJean.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^12.0",
    "laravel/framework": "^12.0",
    "ext-json": "*"
  },
  "require-dev": {
    "pestphp/pest": "^2.0",
    "orchestra/testbench": "^8.0",
    "laravel/nova": "*"
  },
  "autoload": {
    "psr-4": {
      "MarcinJean\\WalletPass\\": "src/WalletPass/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MarcinJean\\WalletPass\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MarcinJean\\WalletPass\\WalletPassServiceProvider"
      ],
      "aliases": {
        "WalletPass": "MarcinJean\\WalletPass\\Facades\\WalletPass"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}